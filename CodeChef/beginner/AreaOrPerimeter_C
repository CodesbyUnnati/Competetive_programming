#include <stdio.h>
#include <string.h>

int main(void) {
  int l,b;
  scanf("%d\n",&l);
  scanf("%d",&b);
  int peri = 2 * (l + b);
  int area = (l * b);
  if(peri > area)
  {
    printf("Peri\n%d",peri);
  }
  else if(area > peri)
  {
    printf("Area\n%d",area);
  }
  else
  {
    printf("Eq\n%d",peri);
  }
  return 0;
}
