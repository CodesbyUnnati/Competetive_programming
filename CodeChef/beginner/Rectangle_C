#include <stdio.h>

int main(void) {
  int t;
  scanf("%d",&t);
  while(t --)
  {
    int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    if(a == b && d == c)
    {
      printf("YES");
    }
    else if(a == c && b == d)
    {
      printf("YES");
    }
    else if(a == d && b == c)
    {
      printf("YES");
    }
    else if(a == b && b == c && c == d)
    {
      printf("YES");
    }
    else
    {
      printf("NO");
    }
    printf("\n");
  }
  return 0;
}
