#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    scanf("%d",&n);
    long long int arr[n];
    int i;
    for(i = 0;i < n;i++)
    {
        scanf("%lld",&arr[i]);
    }
    long long int sum = 0;
    for(i = 0;i < n;i++)
    {
        sum += arr[i];
    }
    printf("%lld",sum);
    return 0;
}
